{% extends 'base.html.twig' %}

{% block title %}Time{% endblock %}

{% block body %}
    <h1>Time</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ time.id }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ time.start ? time.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ time.end ? time.end|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Project_id</th>
                <td>{{ time.projectId }}</td>
            </tr>
            <tr>
                <th>User_id</th>
                <td>{{ time.userId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('time_index') }}">back to list</a>

    <a href="{{ path('time_edit', {'id': time.id}) }}">edit</a>

    {{ include('time/_delete_form.html.twig') }}
{% endblock %}
